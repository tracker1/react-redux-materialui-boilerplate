  },
  "homepage": "https://github.com/tracker1/react-redux-materialui-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
