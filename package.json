    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-modern-node": "^3.2.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-sinon": "^2.8.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "nyc": "^8.3.2",
    "postcss-loader": "^1.1.1",
    "preact": "^6.4.0",
    "preact-compat": "^3.9.2",
    "proxyquire": "^1.7.10",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.7",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "codecov": "^1.0.1",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.1",
    "fbjs": "^0.7.2",
    "fclone": "^1.0.10",
    "glob": "^7.1.1",
    "helmet": "^3.1.0",
    "hpp": "^0.2.1",
    "ignore-styles": "^5.0.1",
    "lodash": "^4.17.2",
    "mz": "^2.5.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "shelljs": "^0.7.5"
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "./scripts/test/init.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "all": true,
    "check-coverage": true
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel-node scripts/build.js",
    "build-client": "cross-env NODE_ENV=production babel-node scripts/build-client.js",
    "build-server": "cross-env NODE_ENV=production babel-node scripts/build-server.js",
    "lint": "eslint ./src/",
    "pretest": "npm run lint && node scripts/pretest.js",
    "test": "cross-env NODE_ENV=test nyc mocha -R spec \"./src/**/*.test.js\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "cross-env NODE_ENV=development babel-node src/server/index.js"
  }
}
